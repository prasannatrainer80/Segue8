<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC 
"-//SPRING//DTD BEAN//EN" 
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="beanPerson1" class="com.hexaware.person.Person">
		<property name="id" value="1" />
		<property name="name" value="Sunitha" />
		<property name="jobDetails">
			<bean class="com.hexaware.person.JobDetails">
				<property name="job" value="Software Engineeer" />
				<property name="company" value="Hexaware" />
				<property name="location" value="Chennai" />
			</bean>
		</property>
		<property name="familyDetails">
			<bean class="com.hexaware.person.Family">
				<property name="city" value="Hyderabad" />
				<property name="state" value="TS" />
			</bean>
		</property>
	</bean>
	
	<bean id="beanPerson2" class="com.hexaware.person.Person">
